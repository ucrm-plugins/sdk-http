{
    "name": "ucrm-plugins/sdk-http",
    "description": "A HTTP library for the UCRM Plugin SDK.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@mvqn.net"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "~7.3",

        "ext-curl": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "ext-pdo": "*",
        "ext-intl": "*",

        "mvqn/data": "dev-master",
        "mvqn/dynamics": "dev-master",
        "mvqn/rest": "dev-master",

        "slim/slim": "^4.7.1",
        "slim/twig-view": "^3.2.0",
        "php-di/php-di": "^6.3.0",
        "slim/psr7": "^1.3.0",


        "ucrm-plugins/sdk-common": "dev-master",
        "ucrm-plugins/sdk-logging": "dev-master",

        "nette/php-generator": "~3.2",
        "defuse/php-encryption": "~2.2",
        "vlucas/phpdotenv": "~2.5",
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~9"
    },
    "autoload": {
        "psr-4": {
            "UCRM\\HTTP\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "./examples/src/"
        }
    },
    "scripts": {
    }

}
